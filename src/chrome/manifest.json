{
	"manifest_version": 2,
	"name": "SaveThisSession",
	"version": "1.0",
	"default_locale": "en",
	"description":  "Save and manage easily your tabs and windows on google chrome.",
	"browser_action": {
		"default_title": "SaveThisSession",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["scripts/background.js"],
		"persistent": false
	},
	"options_page": "options.html",
	"permissions": []
}